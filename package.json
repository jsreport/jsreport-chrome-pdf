{
  "name": "jsreport-chrome-pdf",
  "version": "1.2.0",
  "description": "jsreport recipe which is rendering pdf from html using headless chrome",
  "scripts": {
    "start": "jsreport-studio-start --httpPort 5488",
    "run": "npm run build && jsreport-studio-start --httpPort 5488 --run-only",
    "test": "mocha test --timeout 15000 && standard",
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-chrome-pdf",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-chrome-pdf.git"
  },
  "keywords": [
    "jsreport",
    "pdf"
  ],
  "standard": {
    "ignore": [
      "main.js",
      "main.js.map"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "engines": {
    "node": ">=8.9"
  },
  "license": "MIT",
  "dependencies": {
    "node.extend.without.arrays": "1.1.5",
    "uuid": "3.3.2"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "8.2.5",
    "eslint": "4.19.1",
    "eslint-config-standard-jsx": "5.0.0",
    "eslint-plugin-babel": "5.1.0",
    "in-publish": "2.0.0",
    "jsreport-core": "2.3.0",
    "jsreport-debug": "2.0.2",
    "jsreport-handlebars": "2.0.0",
    "jsreport-studio-dev": "1.3.2",
    "jsreport-templates": "2.1.0",
    "mocha": "5.2.0",
    "parse-pdf": "1.0.0",
    "puppeteer": "1.11.0",
    "should": "13.2.3",
    "standard": "11.0.1"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ]
}
